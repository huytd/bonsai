<!DOCTYPE html>
<html>
    <head>
        <title>Bonsai</title>
        <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
        <%= stylesheet_link_tag    'font/typicons', media: 'all', 'data-turbolinks-track' => true %>
        <link href='https://fonts.googleapis.com/css?family=Open+Sans:400,700,600,300italic,300,400italic,600italic,700italic&subset=latin,vietnamese' rel='stylesheet' type='text/css'>
        <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
        <%= csrf_meta_tags %>
        <meta content="width=device-width, initial-scale=1" name="viewport">
    </head>
    <body>
        <%= render "layouts/header" %>
        <div class="container">
        <%= yield %>
        </div>
    </body>
</html>
